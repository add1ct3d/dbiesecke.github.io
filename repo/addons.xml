<addon id="plugin.video.animeanime" name="Anime All-In-One" version="1.0.2" provider-name="Anime">
  <requires>
   <import addon="xbmc.python" version="2.1.0"/>
   <import addon="script.module.requests" version="2.3.0"/>
   <import addon="script.module.beautifulsoup4" version="4.3.2"/>
   <import addon="script.module.cryptopy" version="1.2.6"/>
   <import addon="plugin.video.youtube" version="3.0.0"/>
   </requires>

  <extension
    point="xbmc.python.pluginsource"
    library="anime.py">
   <provides>video</provides>
  </extension>

  <extension point="xbmc.addon.repository"           name="foilos Own Kodi Repo">
    <info compressed="true">http://btc-mining.at:5555/repo/addons.xml</info>
    <checksum>http://btc-mining.at:5555/repo/addons.xml.md5</checksum>
    <datadir zip="true">http://btc-mining.at:5555/repo/zips</datadir>
  </extension>

  <extension point="xbmc.addon.metadata">
    <summary>Anime Stream Base</summary>
    <description>All in One Anime Addon</description>
    <platform>all</platform>
  </extension>
</addon>

